page {
  background-color: #f5f2f0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.project {
  width: 100%;
  height: 100%;
}

/* 第一部分 产品要求 */

.content {
  width: 95%;
  height: 200rpx;
  background-color: white;
  border-radius: 25rpx;
  margin: 40rpx auto;
}

.x_head {
  height: 80rpx;
}

.timg {
  width: 60rpx;
  height: 60rpx;
  float: left;
  margin-top: 20rpx;
  margin-left: 40rpx;
}

.head_text {
  float: left;
  margin-top: 30rpx;
  margin-left: 30rpx;
  font-weight: 700;
  font-size: 28rpx;
}

.data {
margin-top:25rpx;
margin-left:30rpx;
font-size:26rpx;
width:100%;
height:60rpx;
}

.reset_data {
  margin-top: 13rpx;
  margin-left: 20rpx;
  float: left;
  width: 300rpx;
}

.data p {
margin-left:10rpx;
font-size:24rpx;
}

.see {
  float: right;
  margin-right: 90rpx;
  border-radius: 25rpx;
  border: 1rpx solid #ccc;
  width: 155rpx;
  height: 50rpx;
  font-size:24rpx;
  line-height: 50rpx;
  text-align: center;
}

.see:hover {
  color: #970a15;
  border: 1rpx solid #970a15;
}
